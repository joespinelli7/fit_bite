<%=form_for(@user_activity) do |f|%>
<%=f.label "Edit activity"%>
<%=f.collection_select :activity_id, Activity.all, :id, :name, {:prompt => true}%><br>
<%=f.label :mins_active %>
<%=f.select :mins_active,options_for_select(20..240) %><br>
 <div><%=f.submit "Edit #{@user_activity.activity.name}"%></div>
<%end%><br>
